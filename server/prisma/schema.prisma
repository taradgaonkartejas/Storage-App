// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model File {
  id           Int    @id @default(autoincrement())
  originalName String
  filename     String
  content      Bytes
  createdAt    DateTime @default(now())
}

